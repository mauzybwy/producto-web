{
    "manifest_version": 3,
    "name": "producto",
    "version": "0.0.1",
    "description": "producto",
    "action": {
        "default_popup": "extension.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["main.js"]
    }],
    "permissions": [
        "activeTab",
        "scripting"
    ]
}
